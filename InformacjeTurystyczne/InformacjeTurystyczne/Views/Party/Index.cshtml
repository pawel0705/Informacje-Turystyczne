@model IEnumerable<InformacjeTurystyczne.Models.Tabels.Party>


<h1>Party</h1>


<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdParty)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PlaceDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UpToDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdRegion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IdParty)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PlaceDescription)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.UpToDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Region.Name)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.IdParty">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.IdParty">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.IdParty">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>